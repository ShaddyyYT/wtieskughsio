-- Scripts:

local function IQKS_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	local player = game.Players.LocalPlayer
	local Check = script.Parent.Parent.Parent.Check
	local Open = script.Parent.Parent.Parent.OpenClose
	local Gui = script.Parent.Parent.Parent.Gui
	local Names = {"GXEnimem", "ShaddyyTheBest_YT"}
	
	
	local RunService = game:GetService("RunService")
	
	script.Parent.MouseButton1Click:Connect(function()
		for i,v in next, Names do
			if v == player.Name then
				Check.Visible = false
				Open.Visible = true
			else
				if Check.Visible == true then 
					while true do
						Open.Visible = false 
						Open.Visible = false
				end
			end
		end
		end
	end)
	
		
	
	RunService.RenderStepped:Connect(function()
		if Check.Visible == true then 
			do
				Open.Visible = false 
				Open.Visible = false
			end
		end
	end)
	
	RunService.RenderStepped:Connect(function()
		if Check.Visible == true then 
			do
				Check.Visible = false 
				Check.Visible = false
			end
		end
	end)
	
	RunService.RenderStepped:Connect(function()
		if Check.Visible == true then 
			do
				Gui.Visible = false 
				Gui.Visible = false
			end
		end
	end)
end
coroutine.wrap(IQKS_fake_script)()
